package temp;

import java.io.File;
import java.io.IOException;
import java.util.Scanner;
import static java.lang.System.*;
public class ex01 {
	public static void main(String[] args) {
		Scanner sc = null;
		try {
			sc = new Scanner(new File("qdv.txt"));
		} catch (IOException e) {
			out.print("Erro no ficheiro "+ e);
		}
	}
}
