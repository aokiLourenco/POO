		Scanner sc = null;
		TreeMap<Integer,Double> dados = new TreeMap<Integer,Double>();
		
		try {
			sc = new Scanner(new File("notas.txt"));
		} catch (IOException e) {
			System.out.print("ERRO"+e);
		}
		
		while (sc.hasNextLine()) {
			String linha = sc.nextLine();
			String dadosLinha [] = linha.split("\t");
			
			
		}
	}
}
