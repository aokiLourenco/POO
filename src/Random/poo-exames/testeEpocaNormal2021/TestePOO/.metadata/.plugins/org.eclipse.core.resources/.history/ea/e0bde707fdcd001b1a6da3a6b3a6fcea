package ex03;

public class Student {
	private int nmec;
	private String nome;
	private double atp;
	private double ac;
	private double ap;
	
	public Student(int nmec, String nome, double atp, double ac, double ap) {
		this.nmec = nmec;
		this.nome = nome;
		this.atp = atp;
		this.ac = ac;
		this.ap = ap;
	}
	
	
}
